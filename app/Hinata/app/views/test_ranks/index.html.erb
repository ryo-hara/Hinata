<p id="notice"><%= notice %></p>

<h1>Scores</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>ScoreID</th>
      <th>Value</th>
      <th>CREATED_AT</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.id %></td>
        <td><%= score.score_id %></td>
        <td><%= score.value %></td>
        <td><%= score.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

